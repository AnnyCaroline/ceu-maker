#include "arduino/arduino.ceu"

output bool PIN_11;
output bool PIN_12;

code/await Blink (var int pin, var int ms) -> FOREVER do
    loop do
        _digitalWrite(pin,HIGH);
        await (ms)ms;
        _digitalWrite(pin,LOW);
        await (ms)ms;
    end
end

spawn Blink(11, 1000);
spawn Blink(12,  500);

await FOREVER;
