#include "arduino/arduino.ceu"

output bool PIN_04;
output bool PIN_05;
output bool PIN_06;

par do
    loop do
        emit PIN_04(HIGH);
        await 1s;
        emit PIN_04(LOW);
        await 1s;
    end
with
    loop do
        emit PIN_05(HIGH);
        await 500ms;
        emit PIN_05(LOW);
        await 500ms;
    end
with
    loop do
        emit PIN_06(HIGH);
        await 250ms;
        emit PIN_06(LOW);
        await 250ms;
    end
end
